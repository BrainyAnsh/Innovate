//To find the greatest number in any given or inserted Array
import java.util.*;
public class SDA1
{
  public static void main(String args[])
  {
    Scanner in=new Scanner(System.in);
    int n[]=new int[10];
    System.out.println("Enter the elements in the array");
    for(int i=0;i<=10;i++)
    {
      n[i]=in.nextInt();
    }
    int max=0;
    for(int i=0;i<10;i++)
    {
      if(n[i]>max)
      {
        max=n{i];
      }
    }
    System.out.println("the Greatest number in the array is :-"+max);
  }
}
